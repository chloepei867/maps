ed":{"line":193,"column":2}},{"source":"src/CustomMap.ts","name":null,"original":{"line":71,"column":0},"generated":{"line":193,"column":9}},{"source":"src/CustomMap.ts","name":null,"original":{"line":71,"column":1},"generated":{"line":193,"column":18}},{"source":"src/CustomMap.ts","name":null,"original":{"line":71,"column":0},"generated":{"line":194,"column":0}},{"source":"src/CustomMap.ts","name":null,"original":{"line":12,"column":0},"generated":{"line":194,"column":1}},{"source":"src/CustomMap.ts","name":null,"original":{"line":12,"column":0},"generated":{"line":194,"column":2}},{"source":"src/CustomMap.ts","name":null,"original":{"line":12,"column":0},"generated":{"line":194,"column":3}},{"source":"src/CustomMap.ts","name":null,"original":{"line":12,"column":13},"generated":{"line":195,"column":0}},{"source":"src/CustomMap.ts","name":null,"original":{"line":12,"column":13},"generated":{"line":195,"column":7}},{"source":"src/CustomMap.ts","name":null,"original":{"line":12,"column":13},"generated":{"line":195,"column":8}},{"source":"src/CustomMap.ts","name":null,"original":{"line":12,"column":13},"generated":{"line":195,"column":17}},{"source":"src/CustomMap.ts","name":null,"original":{"line":12,"column":13},"generated":{"line":195,"column":20}},{"source":"src/CustomMap.ts","name":null,"original":{"line":12,"column":13},"generated":{"line":195,"column":29}}],"sources":{"src/CustomMap.ts":"import { Loader } from \"@googlemaps/js-api-loader\";\n\n// instructions to every class on how they can be an argument to addMarker method.\nexport interface Mappable {\n  location: {\n    lat: number;\n    lng: number;\n  };\n  markerContent(): string;\n}\n\nexport class CustomMap {\n  //a reference to google map instance\n  private googleMap!: google.maps.Map;\n  private AdvancedMarkerElement!: typeof google.maps.marker.AdvancedMarkerElement;\n  private InfoWindow!: google.maps.InfoWindow;\n\n  public ready: Promise<void>;\n  public divId: string;\n\n  constructor(divId: string) {\n    this.divId = divId;\n    this.ready = this.initGoogleMap();\n  }\n\n  public async addMarker(mappable: Mappable) {\n    await this.ready;\n    const marker = new this.AdvancedMarkerElement({\n      map: this.googleMap,\n      position: mappable.location,\n    });\n\n    //add a info window\n    const infowindow = new google.maps.InfoWindow({\n      content: `${mappable.markerContent()}`,\n    });\n    marker.addListener(\"click\", () => {\n      infowindow.open({\n        anchor: marker,\n        map: this.googleMap,\n      });\n    });\n  }\n\n  private async initGoogleMap(): Promise<void> {\n    const loader = new Loader({\n      // apiKey: import.meta.env.GOOGLE_MAP_API_KEY!,\n      apiKey: \"AIzaSyBHQryCr60rdLYFsVOBEcPpU4Ei4QMGw_o\",\n      version: \"weekly\",\n    });\n\n    try {\n      // load libraries\n      const { Map, InfoWindow } = await loader.importLibrary(\"maps\");\n      const { AdvancedMarkerElement } = await loader.importLibrary(\"marker\");\n      this.googleMap = new Map(\n        document.getElementById(this.divId) as HTMLElement,\n        {\n          // center: { lat: 37.439712, lng: -122.0096919 },\n          center: { lat: 0, lng: 0 },\n          zoom: 1,\n          mapId: \"5c76fbc3540f015f5c453b67\",\n        }\n      );\n\n      this.AdvancedMarkerElement = AdvancedMarkerElement;\n    } catch (e) {\n      console.error(\"Failed to initialize map:\", e);\n    }\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"66b46ed5573c4156163ea99883ec3e1e","cacheData":{"env":{}}}